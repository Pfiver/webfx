<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='naga'>

    <!-- Inherits the Logging API and setting logging properties    -->
    <inherits name="com.google.gwt.logging.Logging"/>
    <set-property name="gwt.logging.logLevel" value="INFO"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>

    <!-- Specify the paths for translatable code                    -->
    <source path="naga"/>         <!-- source path to naga-core and naga-gwt -->
    <super-source path="super"/>  <!-- super-source path to the customized java.util.ServiceLoader -->

</module>
