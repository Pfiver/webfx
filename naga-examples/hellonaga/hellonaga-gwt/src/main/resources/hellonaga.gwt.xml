<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='hellonaga'>

    <!-- Inherit the naga module (nage-core + naga-gwt stuff)       -->
    <inherits name="naga"/> <!-- refers to naga.gwt.xml in naga-gwt -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='hellonaga.gwt.HelloNagaGwtApplication'/>

    <!-- Specify the paths for translatable code                    -->
    <source path="hellonaga"/> <!-- code is under hellonaga package -->

</module>
