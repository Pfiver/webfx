    <inherits name="elemental2.dom.Dom"/>
    <inherits name="com.googlecode.gwt.charts.Charts" />

    <!-- Inherit the WebFx modules  -->
    <inherits name="webfx-platform-gwt"/>

    <inherits name="webfx-fxkits-core"/>

    <inherits name="webfx-fxkits-emul-javafxbase"/>
    <inherits name="webfx-fxkits-emul-javafxgraphics"/>
