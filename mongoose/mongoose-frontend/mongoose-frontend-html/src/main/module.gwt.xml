<module rename-to="mongoose_frontend_html">

    <!-- Speeding up GWT compilation time in development mode -->
    <set-property name="user.agent" value="safari"/> <!-- to make only 1 permutation for testing with Chrome -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='mongoose.html.frontend.activities.application.MongooseFrontendHtmlApplication'/>

    <source path="mongoose/client/frontend/html"/>

</module>
