<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='mongoose_frontend'>

    <!-- Inherit the naga module (naga-core + naga-gwt stuff)       -->
    <inherits name="naga-gui-gwt"/> <!-- refers to naga.gwt.xml in naga-plat-gwt -->

    <inherits name="com.google.gwt.resources.Resources" />

    <!-- Specify the app entry point class.                         -->
    <entry-point class='mongoose.frontend.gwt.MongooseFrontendGwtApplication'/>

    <!-- Specify the paths for translatable code                    -->
    <source path="mongoose"/>   <!-- code is under mongoose package -->
    <resource path="mongoose"/> <!-- for mongoose/domainmodel/DomainModelSnapshot.lzb64json -->
</module>
