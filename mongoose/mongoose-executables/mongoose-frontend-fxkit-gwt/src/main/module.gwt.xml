<module rename-to="mongoose_frontend_fxkit_gwt">

    <!-- Configuring logging (must be in first position) -->
    <inherits name="com.google.gwt.logging.Logging" />
    <set-property name="gwt.logging.enabled" value="TRUE" />
    <set-property name="gwt.logging.logLevel" value="FINEST" />
    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />

    <inherits name="mongoose-frontend-application"/>
    <inherits name="webfx-fxkit-gwt"/>
    <inherits name="webfx-platforms-web-clientbus"/>
    <inherits name="webfx-platform-gwt-appcontainer"/>
    <inherits name="webfx-platform-gwt-json"/>
    <inherits name="webfx-platform-gwt-uischeduler"/>
    <inherits name="webfx-platform-gwt-log"/>
    <inherits name="webfx-platform-gwt-resource"/>
    <inherits name="webfx-platform-gwt-shutdown"/>
    <inherits name="webfx-platform-gwt-storage"/>
    <inherits name="webfx-platform-gwt-websocket"/>
    <inherits name="webfx-platform-gwt-windowlocation"/>
    <inherits name="webfx-platform-gwt-windowhistory"/>
    <inherits name="webfx-platform-gwt-super"/>
    <inherits name="webfx-lib-controlsfx-validation" />
    <inherits name="webfx-lib-javacupruntime" />

    <!-- Speeding up GWT compilation time in development mode -->
    <set-property name="user.agent" value="safari"/> <!-- to make only 1 permutation for testing with Chrome -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class="webfx.platform.gwt.services.appcontainer.GwtApplicationContainerProvider"/>

    <super-source path="super"/>

</module>
