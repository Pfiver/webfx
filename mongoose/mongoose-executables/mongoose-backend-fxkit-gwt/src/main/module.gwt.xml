<module rename-to="mongoose_backend_fxkit_gwt">

    <!-- Configuring logging (must be in first position) -->
    <inherits name="com.google.gwt.logging.Logging" />
    <set-property name="gwt.logging.enabled" value="TRUE" />
    <set-property name="gwt.logging.logLevel" value="FINEST" />
    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />

    <inherits name="mongoose-backend-application"/>
    <inherits name="webfx-fxkit-gwt"/>
    <inherits name="webfx-platform-client-websocketbus-web"/>
    <inherits name="webfx-platform-shared-appcontainer-gwt"/>
    <inherits name="webfx-platform-shared-json-gwt"/>
    <inherits name="webfx-platform-client-uischeduler-gwt"/>
    <inherits name="webfx-platform-shared-log-gwt"/>
    <inherits name="webfx-platform-shared-resource-gwt"/>
    <inherits name="webfx-platform-shared-shutdown-gwt"/>
    <inherits name="webfx-platform-client-storage-gwt"/>
    <inherits name="webfx-platform-client-websocket-gwt"/>
    <inherits name="webfx-platform-client-windowlocation-gwt"/>
    <inherits name="webfx-platform-client-windowhistory-gwt"/>
    <inherits name="webfx-platform-gwt-super"/>
    <inherits name="webfx-lib-controlsfx-validation" />
    <inherits name="webfx-lib-javacupruntime" />

    <!-- Speeding up GWT compilation time in development mode -->
    <set-property name="user.agent" value="safari"/> <!-- to make only 1 permutation for testing with Chrome -->

    <!-- Specify the app entry point class. -->
    <entry-point class="webfx.platform.gwt.services.appcontainer.GwtApplicationContainerProvider"/>

    <super-source path="super"/>

</module>
