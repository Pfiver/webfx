<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='webfx_javafxport_gwt'>

    <inherits name="elemental2.core.Core"/>
    <inherits name="elemental2.dom.Dom"/>
    <inherits name="com.googlecode.gwt.charts.Charts" />

    <!-- Inherit the naga module (naga-core + naga-gwt stuff)       -->
    <inherits name="webfx-platformport-gwt"/> <!-- refers to webfx-platformport-gwt.gwt.xml in webfx-platformport-gwt -->
    <inherits name="naga-type"/>

    <inherits name="naga-javaemul-javafxbase"/>
    <inherits name="naga-javaemul-javafxgraphics"/>
    <inherits name="naga-javaemul-javafxcontrols"/>

    <!-- Specify the paths for translatable code                    -->
    <source path="naga/fx/spi/gwt"/>               <!-- source path to webfx-javafxport-gwt -->
    <super-source path="super"/>        <!-- super-source path to the customized jdk code (not covered by GWT by default) -->
</module>
