<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='naga_toolkit_html'>

    <inherits name="elemental2"/>

    <!-- Inherit the naga module (naga-core + naga-gwt stuff)       -->
    <inherits name="naga-platform-gwt"/> <!-- refers to naga-platform-gwt.gwt.xml in naga-platform-gwt -->
    <inherits name="naga-toolkit"/> <!-- refers to naga-platform-gwt.gwt.xml in naga-platform-gwt -->

    <!-- Specify the paths for translatable code                    -->
    <source path="naga/providers/toolkit/html"/>               <!-- source path to naga-core and naga-platform-gwt -->
    <super-source path="super"/>        <!-- super-source path to the customized jdk code (not covered by GWT by default) -->
</module>
